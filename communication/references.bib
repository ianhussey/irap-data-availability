
@article{hussey_implicit_2020,
	title = {The {Implicit} {Relational} {Assessment} {Procedure} demonstrates poor internal consistency and test-retest reliability: {A} meta-analysis},
	url = {https://psyarxiv.com/ge3k7/},
	doi = {10.31234/osf.io/ge3k7},
	abstract = {Evidence for the IRAP’s reliability and validity is mixed, with one meta-analysis concluding it has good criterion validity and potential for clinical assessment, and two others concluding that it demonstrates low reliability. Here, we extend this evidence base through meta-analyses of all published and unpublished studies conducted in two labs. Individual participant data was used to estimate both internal consistency and test-retest reliability across a large number of domains (k = 16) and participants (N = 1576). Results suggest that internal consistency is poor (α = .51, 95\% CI [.46, .56]) and test-retest reliability is very poor (ICC = .20, 95\% CI [.05, .34]). We conclude that researchers should be very cautious about choosing to employ the IRAP or when interpreting its results.},
	urldate = {2020-06-11},
	journal = {PsyArXiv},
	author = {Hussey, Ian and Drake, Chad E.},
	year = {2020},
	note = {Publisher: PsyArXiv},
	file = {Hussey and Drake - 2020 - The Implicit Relational Assessment Procedure demon.pdf:/Users/Ian-Hussey/Zotero/storage/BZY2CWTZ/Hussey and Drake - 2020 - The Implicit Relational Assessment Procedure demon.pdf:application/pdf},
}

