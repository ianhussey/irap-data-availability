	keywords = {Assessment, behavior analysis, Clinical Psychology, implicit measures, implicit relational assessment procedure, meta-science, Meta-science, relational frame theory, Social and Behavioral Sciences, Social and Personality Psychology, social cognition, Social Cognition, systematic review},
